    "homepage": "http://framework.zend.com/",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^0.1.0",
        "zendframework/zend-config": "^2.6",
        "zendframework/zend-mvc": "^3.0.0@dev",
        "zendframework/zend-json-server": "^3.0",
        "zendframework/zend-xml2json": "^3.0",
        "zendframework/zend-mvc-plugin-flashmessenger": "^0.1.0",
        "zendframework/zend-form": "^2.8",
        "zendframework/zend-crypt": "^2.6",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-i18n-resources": "^2.5",
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-authentication": "^2.5",
        "zendframework/zend-paginator": "^2.7",
        "crsoares/doctrine-orm-module": "dev-master",
        "crsoares/doctrine-module": "dev-master",
        "gedmo/doctrine-extensions": "^2.4",
        "beberlei/DoctrineExtensions": "^1.0",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "crsoares/assetmanager": "dev-master",
        "crsoares/cr-module-layouts": "dev-master"
